<div class="form-container">
  <div class="form-wrapper">
    <form action="<%= git_hub_show_path %>" method="get">
      <div class="mb-3 text-center">
        <label for="exampleInputLogin" class="form-label">Please, enter login from GitHub</label>
        <input type="text" class="form-control form-input" id="exampleInputLogin" aria-describedby="loginHelp" name="login">
      </div>
      <div class="text-center">
        <button type="submit" class="btn submit-button">Submit</button>
      </div>
    </form>
  </div>
</div>
